# Multi-Agent Observability Configuration

# Server Configuration
OBSERVABILITY_SERVER_URL=http://localhost:4000
OBSERVABILITY_PORT=4000
OBSERVABILITY_ENABLED=true
OBSERVABILITY_TIMEOUT=5

# API Security (Optional)
OBSERVABILITY_API_KEY=

# Database Configuration
DATABASE_PATH=./events.db
DATABASE_BACKUP_ENABLED=true
DATABASE_BACKUP_INTERVAL=24

# Agent Detection Configuration
AGENT_DETECTION_METHOD=auto
CLAUDE_AGENT_NAME=

# WebSocket Configuration
WEBSOCKET_ENABLED=true
WEBSOCKET_MAX_CLIENTS=100

# Security Configuration
SECURITY_MONITORING_ENABLED=true
DANGEROUS_COMMAND_DETECTION=true
ENV_FILE_PROTECTION=true

# Dashboard Configuration
DASHBOARD_ENABLED=true
DASHBOARD_AUTO_REFRESH=true
DASHBOARD_THEME=default

# Logging Configuration
LOG_LEVEL=info
LOG_RETENTION_DAYS=30
ERROR_LOG_ENABLED=true

# Performance Configuration
EVENT_BATCH_SIZE=100
EVENT_RETENTION_DAYS=90
STATS_CACHE_TTL=300